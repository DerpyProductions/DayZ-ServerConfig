  "RuleCategories": [
    {
      "CategoryName": "General Rules",
      "Rules": [
        {
          "RuleParagraph": "1.1.",
          "RuleText": "Be Respectful, Mature & Friendly! Insults, discrimination, extremist and racist statements or texts are taboo."
        },
        {
          "RuleParagraph": "1.2.",
          "RuleText": "We reserve the right to exclude people from the server who share extremist or racist ideas or who clearly disturb the server harmony."
        },
        {
          "RuleParagraph": "1.3.",
          "RuleText": "PvE ONLY unless in designated PvP Zones. You can shoot in and out of PvP Zones."
        },
        {
          "RuleParagraph": "1.4.",
          "RuleText": "No Cheating / Hacking / Glitching / Duping of any form."
        },
        {
          "RuleParagraph": "2.1.",
          "RuleText": "Your Base-Building-Style should follow the Laws of Physics!"
        },
        {
          "RuleParagraph": "2.2.",
          "RuleText": "No Base Raiding of any kind! No theft of any kind!"
        },
        {
          "RuleParagraph": "3.1.",
          "RuleText": "Do not park your Car anywhere on the Map for more than 1 Server restart."
        },
        {
          "RuleParagraph": "3.2.",
          "RuleText": "Use the virtual Garage for your Vehicles. Garages are located at every Gas Station."
        },
        {
          "RuleParagraph": "3.3.",
          "RuleText": "Please don't rush through or over a PVP Zone with AI spawn locations (particularly Tisy, NWAF and Bandit Camp Skalka) in a Vehicle. This triggers the AI spawn unnecessarily and severely degrades server performance!"
        },
        {
          "RuleParagraph": "4.1.",
          "RuleText": "Have Fun!"
        },
        {
          "RuleParagraph": "4.2.",
          "RuleText": "Failure to comply with any Rules will result in a Ban. No Warning may be given!"
        }
      ]
    }
  ],
